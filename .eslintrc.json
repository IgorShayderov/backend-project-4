{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns":["/node_modules/*"],
  "rules": {
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "max-len": ["error", { "code": 100, "tabWidth": 2 }]
  }
}
